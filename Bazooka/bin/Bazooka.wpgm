import wollok.game.*
import Batallas.*
import Personajes.*
import Enemigos.*
import MenuBatalla.*
import Mapas.*
import Inicio.*


program Bazooka {
	const inicioSong = game.sound("song/inicio.mp3")
	const mapa1Song = game.sound("song/mapa1.mp3")
	
	inicioSong.shouldLoop(true)
	mapa1Song.shouldLoop(true)
	
	game.schedule(10, { inicioSong.play()} )
	keyboard.enter().onPressDo({if (inicio.compruebaInicioSong()){
		inicioSong.stop()
		mapa1Song.play()
	}})
	
	
	
 	inicio.set()
 	
	
	
}


	